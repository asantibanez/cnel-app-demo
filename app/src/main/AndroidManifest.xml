<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.andressantibanez.cnelapp" >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="com.andressantibanez.android.cnelapp.CnelApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.andressantibanez.android.cnelapp.checkmeter.MeterSearchActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.andressantibanez.android.cnelapp.checkpayments.CheckPaymentsActivity"
            android:label="@string/title_activity_check_payments" >
        </activity>
        <activity
            android:name="com.andressantibanez.android.cnelapp.checkconsumption.CheckConsumptionsActivity"
            android:label="@string/title_activity_check_consumptions" >
        </activity>
    </application>

</manifest>
